<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="liquor_order_line_view_tree" model="ir.ui.view">
        <field name="name">liquor_order_line_view_tree</field>
        <field name="model">liquor.order.line</field>
        <field name="arch" type="xml">
            <tree string="liquor_order_line_view_tree">
                <field name="product_id"/>
                <field name="quantity"/>
                <field name="price"/>
            </tree>
        </field>
    </record>
    <record id="liquor_order_line_action" model="ir.actions.act_window">
        <field name="name">liquor order Line</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">liquor.order.line</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>